# 生产环境配置
NODE_ENV=production

# Gateway Rust API 配置
VITE_API_BASE_URL=/
VITE_WS_BASE_URL=ws://localhost:50010

# 应用配置
VITE_APP_TITLE=Gateway Rust - IoT设备网关管理
VITE_APP_VERSION=1.0.0
VITE_APP_DESCRIPTION=IoT设备网关Web管理界面

# WebSocket配置
VITE_WS_HEARTBEAT_INTERVAL=30000
VITE_WS_RECONNECT_INTERVAL=5000
VITE_WS_MAX_RECONNECT_ATTEMPTS=10

# 生产配置
VITE_DEBUG=false
VITE_SHOW_CONSOLE_LOGS=false
