# 开发调试环境配置 - 端口50000+
NODE_ENV=development

# 前端开发服务器端口
VITE_DEV_PORT=50020

# 后端API地址 - 连接到控制台启动的后端服务
VITE_API_BASE_URL=http://localhost:50010

# WebSocket地址  
VITE_WS_BASE_URL=ws://localhost:50016

# 第三方服务地址 - 连接到Docker中的服务
VITE_INFLUXDB_URL=http://localhost:50001
VITE_GRAFANA_URL=http://localhost:50002
VITE_PROMETHEUS_URL=http://localhost:50003
VITE_EMQX_DASHBOARD_URL=http://localhost:50004
VITE_MQTT_BROKER_URL=ws://localhost:50006

# 调试配置
VITE_DEBUG=true
VITE_LOG_LEVEL=debug

# 应用配置
VITE_APP_TITLE=Gateway Rust - Debug Mode
VITE_APP_VERSION=dev-debug
VITE_APP_BASE_PATH=/